package com.masai.beam;

public class ProjectDTO {

	private int projectId;
	private String projectDesc;
	private String projectDist;
	private String projectVillage;
	
	private int gpmId;
	private String gpmName;
	
	
	
	public ProjectDTO() {
		super();
	}



	public ProjectDTO(int projectId, String projectDesc, String projectDist, String projectVillage, int gpmId,
			String gpmName) {
		super();
		this.projectId = projectId;
		this.projectDesc = projectDesc;
		this.projectDist = projectDist;
		this.projectVillage = projectVillage;
		this.gpmId = gpmId;
		this.gpmName = gpmName;
	}



	public int getProjectId() {
		return projectId;
	}



	public void setProjectId(int projectId) {
		this.projectId = projectId;
	}



	public String getProjectDesc() {
		return projectDesc;
	}



	public void setProjectDesc(String projectDesc) {
		this.projectDesc = projectDesc;
	}



	public String getProjectDist() {
		return projectDist;
	}



	public void setProjectDist(String projectDist) {
		this.projectDist = projectDist;
	}



	public String getProjectVillage() {
		return projectVillage;
	}



	public void setProjectVillage(String projectVillage) {
		this.projectVillage = projectVillage;
	}



	public int getGpmId() {
		return gpmId;
	}



	public void setGpmId(int gpmId) {
		this.gpmId = gpmId;
	}



	public String getGpmName() {
		return gpmName;
	}



	public void setGpmName(String gpmName) {
		this.gpmName = gpmName;
	}



	@Override
	public String toString() {
		return "ProjectDTO [projectId=" + projectId + ", projectDesc=" + projectDesc + ", projectDist=" + projectDist
				+ ", projectVillage=" + projectVillage + ", gpmId=" + gpmId + ", gpmName=" + gpmName + "]";
	}
	
	
	
}
